@model IEnumerable<MVCMusicStoreApplication.Models.Genre>
@{
    ViewBag.Title = "Browse";
}

<h2>Browse</h2>

<h2>Browse by Genre</h2>

<ul>
    @foreach (var genre in Model)
    {
        <li>
            @Html.ActionLink(genre.Name, "Index", "Store", new { id = genre.GenreId }, null)
        </li>
    }
</ul>